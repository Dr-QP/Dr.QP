{
    "name": "hexapod",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@serialport/bindings-cpp": "^13.0.1",
        "express": "^4.21.2",
        "hexapod-kinematics-library": "0.0.5",
        "johnny-five": "^2.1.0",
        "plotly.js-gl3d-dist-min": "^3.0.1",
        "rclnodejs": "^0.32.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-icons": "^5.5.0",
        "react-plotly.js": "^2.6.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^5.0.1",
        "serve": "^14.2.4",
        "socket.io": "^4.8.1",
        "socket.io-client": "^3.0.5",
        "web-vitals": "^4.2.4"
    },
    "overrides": {
        "@serialport/bindings": {
            "@serialport/bindings-cpp": "^13.0.1"
        }
    },
    "scripts": {
        "build-prod": "react-scripts build",
        "build-dev": "",
        "test-client": "react-scripts test --watchAll=false --coverage --coverageReporters lcov",
        "test-server": "jest ./server --config jest-server.config.js",
        "test": "yarn test-client && yarn test-server",
        "start-client-dev": "react-scripts start",
        "start-client-prod": "serve -s build; echo Ignoring the args:",
        "start-server": "node ./server/index.js",
        "start-kill-server": "yarn kill-server; yarn start-server",
        "kill-server": "fuser -k 4001/tcp",
        "start-dev": "concurrently -n \"server,client\" \"yarn start-kill-server\" \"yarn start-client-dev\"",
        "start-prod": "concurrently -n \"server,client\" \"yarn start-kill-server {@}\" \"yarn start-client-prod\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^13.5.0",
        "canvas": "^3.1.0",
        "concurrently": "^9.1.2",
        "jest": "^29.7.0",
        "prettier": "3.5.3"
    }
}
