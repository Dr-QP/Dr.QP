{
  "name": "ROS 2 jazzy prebuilt",
  "remoteUser": "rosdev",
  "image": "ghcr.io/dr-qp/jazzy-ros-desktop:edge",
  "customizations": {
    "vscode": {
      "extensions": [
        // Open the /home/rosdev/ros2_ws/Dr.QP.code-workspace to install the recommended extensions
      ]
    }
  },
  "workspaceFolder": "/home/rosdev/ros2_ws/",
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/rosdev/ros2_ws/,type=bind",
  "containerEnv": {},
  "mounts": [
    "type=volume,source=drqp-build,target=/home/rosdev/ros2_ws/build",
    "type=volume,source=drqp-install,target=/home/rosdev/ros2_ws/install",
    "type=volume,source=drqp-log,target=/home/rosdev/ros2_ws/log",
    "type=volume,source=drqp-lcov,target=/home/rosdev/ros2_ws/lcov",
    "type=volume,source=drqp-cache,target=/home/rosdev/ros2_ws/.cache",
    "type=volume,source=drqp-docs-build,target=/home/rosdev/ros2_ws/docs/_build"
  ],
  "runArgs": [
    // Commented out to allow Codespaces use
    // "--network", "host",
    // "--device", "/dev/ttySC0"
  ],
  "features": {
  }
}
