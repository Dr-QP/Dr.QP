{
  "name": "ROS 2 Workspace humble-ros-base",
  "remoteUser": "rosdev",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "USERNAME": "rosdev",
      "GID": "1000",
      "UID": "1000"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        // Open the /home/rosdev/ros2_ws/src/Dr.QP/Dr.QP.code-workspace to install the recommended extensions
      ]
    }
  },
  "workspaceFolder": "/home/rosdev/ros2_ws/src/Dr.QP/",
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/rosdev/ros2_ws/src/Dr.QP/,type=bind",
  "mounts": [
    {
      "type": "bind",
      "source": "${localEnv:HOME}/.1password",
      "target": "/home/rosdev/.1password"
    },
    {
      "type": "bind",
      "source": "${localEnv:HOME}/.ssh",
      "target": "/home/rosdev/.ssh"
    }
  ],
  "runArgs": [
    "--net=host"
  ],
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {},
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {},
    "ghcr.io/devcontainers/features/git:1": {},
    "ghcr.io/devcontainers/features/git-lfs:1": {},
    "ghcr.io/dhoeric/features/act:1": {},
    "ghcr.io/itsmechlark/features/1password": {},
    "ghcr.io/devcontainers-community/features/llvm": {
      "version": "19"
    },
    "ghcr.io/devcontainers-extra/features/cmake": {},
    "ghcr.io/devcontainers/features/node": {},
    "ghcr.io/devcontainers-extra/features/fish-apt-get": {},
  }
}
